<h4>Click on the movie to check the showtimes!</h4>
<a href="/">
  <p>Back</p>
</a>

<div id="movie-box">
  {{#each movies}}
  <div id="movie-container">
    <img src="{{this.image}}" alt="{{this.title}}">
    <h3>{{this.title}}</h3>
    <a href="/movies/{{this._id}}">
      <button type="submit">See more</button>
    </a>
  </div>
  {{/each}}
</div>